{
  "name": "resume-parser-be-serverless",
  "author": "Amit-Milli",
  "version": "0.1.0",
  "private": true,
  "description": "Resume parser AI agent backend code with serverless framework, lambda, dynamodb,  for authentication, s3, langraph",
  "scripts": {
    "deploy": "stage=test region=us-east-1 serverless deploy",
    "lint": "eslint 'src/**/*.js'",
    "lint:fix": "eslint --fix 'src/**/*.js'",
    "pretest": "rm -rf lint; mkdir lint; eslint --fix 'src/**/*.js' -f html > lint/index.html",
    "pretest:win": "eslint --fix src/**/*.js -f html > lint/index.html",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.23.10",
    "eslint": "^8.57.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.29.1",
    "husky": "^9.0.11",
    "serverless-prune-plugin": "^2.0.1"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.540.0",
    "@aws-sdk/lib-dynamodb": "^3.540.0",
    "@aws-sdk/client-s3": "^3.540.0",
    "@aws-sdk/client-sqs": "^3.540.0",
    "@langchain/core": "^0.3.61",
    "@langchain/openai": "^0.5.15",
    "axios": "^1.7.2",
    "crypto-js": "^4.2.0",
    "langchain": "^0.3.29",
    "multer": "^2.0.1",
    "pdf-parse": "^1.1.1",
    "uuid": "^11.1.0"
  }
}